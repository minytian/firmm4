olor'\">\n    </mbr-parameters>\n    \n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    \n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"footer_col_logo\">\n                <div class=\"logo_wrap\" mbr-if=\"showLogo == 1\">\n                    <a href=\"#\">\n                        <img src=\"../_images/logo.png\" alt=\"Mobirise\">\n                    </a>\n                </div>\n                <div class=\"brand_wrap\" mbr-if=\"showLogo == 2\">\n                    <h3 mbr-theme-style=\"display-5\" class=\"brand_name mb-0\" data-app-selector=\".brand_name\"><b>FirmM4.</b></h3>\n                </div>\n                <div class=\"contacts\" mbr-if=\"showContacts\">\n                    <p class=\"contacts_mail mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts_mail\"><b>E-mail:</b> <a href=\"#\" class=\"text-white\">mobirise@company.com</a></p>\n                    <p class=\"contacts_phone mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts_phone\"><b>Phone:</b> <a href=\"#\" class=\"text-white\">+7 123 456 78 90</a></p>\n                </div>\n                <div mbr-form=\"\" class=\"mbr-form\" mbr-if=\"showForm\">\n                <form action=\"\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                    <div class=\"\">\n                        <div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Thanks for filling\n                            out the form!</div>\n                        <div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">Oops...! some\n                            problem!</div>\n                    </div>\n                    <div class=\"dragArea form_wrap\">\n                        <div class=\